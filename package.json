{
  "name": "interlinqed",
  "version": "1.0.0",
  "description": "A functional implementation of LINQ in TypeScript",
  "author": "iFaxity (christian@faxity.se)",
  "license": "MIT",
  "homepage": "https://ifaxity.github.io/interlinqed",
  "repository": {
    "type": "git",
    "url": "https://github.com/ifaxity/interlinqed.git"
  },
  "bugs": {
    "url": "https://github.com/ifaxity/interlinqed/issues"
  },
  "keywords": [
    "linq",
    "linqts",
    "typescript",
    "c#",
    "array",
    "iterator",
    "generator",
    "filter"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "tsc --noEmit",
    "docs": "typedoc src",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/node": "^18.0.0",
    "c8": "^7.11.3",
    "typedoc": "^0.22.17",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.2"
  }
}
